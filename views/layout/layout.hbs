    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.8/css/line.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <link rel="stylesheet" href="stylesheets/{{style}}">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">



    <title>Shopping Cart</title>
    <style>
        nav {
            position: fixed;
            z-index: 1;
        }

        .navbar-light {
            background-color: rgb(255, 255, 255) !important;
        }

        .nav-link:hover {
            border-radius: 5px;
            background: linear-gradient(to right, #ccccff 0%, #ff99cc 100%);
        }

        .giftmenu ul li a {
            text-decoration: none;
            color: black;
        }

        .submenu-1 {
            display: none;

        }

        .giftmenu {

            z-index: 9999;
            position: absolute;
            left: 38%;
            top: 66%;
            width: 125px;
            transform: translate(-50%, -50%);
        }

        .plants {
            list-style-type: none;
            position: absolute;
            left: 191%;
            top: -44%;
            width: 125px;
            transform: translate(-50%, -50%);
        }

        .search {
            position: absolute;
            left: 73%;
            top: 67%;
            transform: translate(-50%, -50%);
            width: 176px;
        }

        .btn-success {
            position: absolute;
            left: 117%;
            top: 25%;
            background-color: pink;
            transform: translate(-50%, -50%);
        }

        .cakes {
            list-style-type: none;
            position: absolute;
            left: 62%;
            width: 125px;
            top: -44%;
            transform: translate(-50%, -50%);
        }

        .Flowers {
            list-style-type: none;
            position: absolute;
            left: 123%;
            width: 125px;
            top: -44%;
            transform: translate(-50%, -50%);
        }

        .Sweets {
            list-style-type: none;
            position: absolute;
            left: -13%;
            top: -44%;
            width: 125px;
            transform: translate(-50%, -50%);
        }

        .giftmenu ul li:hover .submenu-1 li:first-child {
            margin-top: -10px;
        }

        .giftmenu ul li:hover .submenu-1 {
            display: block;
            position: absolute;
            background: linear-gradient(to right, #6666fd 0%, #ff2391 100%);

        }

        .giftmenu ul li:hover .submenu-1 li {
            width: 150px;
            padding: 10px;
            border-bottom: .5px dotted #ffff;
            background: transparent;
            border-radius: 0;
            text-align: left;
        }

        .giftmenu ul li:hover .submenu-1 ul li:last-child {
            border-bottom: none;
        }

        .giftmenu ul li:hover .submenu-1 li a:hover {
            color: rgb(255, 15, 155);
        }

        .fa-angle-right {
            float: right;
        }

        .submenu2 {
            display: none;
        }

        .hover-me:hover .submenu2 {
            position: absolute;
            display: block;
            margin-top: -40px;
            margin-left: 140px;
            background: #6666fd;
        }

        .navbar {
            height: 105px;
            /* set desired height */
        }

        .phone-number {
            margin-top: -83px;
            font-size: 15px;
            font-family: cursive;
        }
    </style>
</head>

<body>
    {{#if admin}}
    {{>admin-header}}
    {{else}}
    {{>user-header}}
    {{/if}}
    {{{body}}}



